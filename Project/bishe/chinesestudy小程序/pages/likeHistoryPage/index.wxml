<view class="container">
  <view class="header">
    <text class="title">点赞记录</text>
  </view>
  <scroll-view scroll-y class="history-list">
    <block wx:for="{{likeHistory}}" wx:key="videoId">
      <navigator url="/pages/videoPage/index?videoId={{item.videoId}}" class="history-item">
        <text class="video-title">{{item.title}}</text>
        <text class="like-time">点赞时间：{{item.likedTime}}</text>
      </navigator>
    </block>
  </scroll-view>
</view> 